{
	"name": "disclip",
	"main": "src/server.js",
	"compatibility_date": "2025-09-21",
	"compatibility_flags": ["nodejs_compat"],
	"browser": {
		"binding": "BROWSER"
	},
	"placement": {
		"mode": "smart"
	},
	"observability": {
		"enabled": true,
		"head_sampling_rate": 1
	},
	"rules": [
		{
			"type": "Text",
			"globs": ["**/*.css"],
			"fallthrough": true
		}
	]
}
